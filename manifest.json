{
  "manifest_version": 2,
  "name": "MyCourses File Saver",
  "description": "Add buttons to save documents on MyCourses to Google Drive and Dropbox",
  "version": "0.0",

  "permissions": [
    "https://mycourses.rit.edu/"
  ],
  
  "content_scripts": [{
    "matches": ["https://mycourses.rit.edu/*"],
    "css": ["css/style.css"],
    "js": ["js/button_injector.js"]
  }]
}